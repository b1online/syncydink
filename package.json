{
  "name": "syncydink",
  "version": "0.0.2",
  "description": "2D and VR Movie Player with Haptics Integration and Synchronization",
  "author": "Kyle Machulis <kyle@machul.is>",
  "private": true,
  "scripts": {
    "trash": "trash dist dist-bundle",
    "start": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "start:secure": "cross-env NODE_ENV=development webpack-dev-server --open --hot --https",
    "build": "yarn trash && cross-env NODE_ENV=production webpack --progress --hide-modules",
    "tslint": "tslint -c tslint.json 'src/**/*.ts' 'tests/**/*.ts'",
    "tslint:fix": "tslint -c tslint.json --fix semicolon,comma,no-var-keyword,ordered-imports 'src/**/*.ts' 'tests/**/*.ts'",
    "webpack-defaults": "webpack-defaults",
    "build:main": "yarn trash && tsc -p tsconfig.json",
    "pretest": "yarn build:main",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha --compilers ts:ts-node/register 'tests/**/*.ts'",
    "deploy": "trash deploy && mkdir -p deploy && cp index.html deploy && cp -r dist deploy"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^17.2.0",
    "aframe": "^0.7.1",
    "aframe-stereo-component": "^0.6.0",
    "buttplug": "^0.6.0",
    "d3": "^4.13.0",
    "haptic-movie-file-reader": "^0.0.10",
    "mousetrap": "^1.6.1",
    "viewport-units-buggyfill": "^0.6.2",
    "vue": "^2.5.13",
    "vue-awesome": "^2.3.4",
    "vue-buttplug-material-component": "^0.1.1",
    "vue-material": "^0.8.1",
    "vue-touch": "next",
    "vue-video-player": "^3.1.4"
  },
  "devDependencies": {
    "@types/aframe": "^0.7.0",
    "@types/chai": "^4.1.2",
    "@types/d3": "^5.0.0",
    "@types/mocha": "^2.2.48",
    "@types/mousetrap": "^1.6.0",
    "@types/node": "^9.4.6",
    "@types/tween.js": "^16.9.0",
    "@types/video.js": "^6.2.3",
    "chai": "^4.1.2",
    "codecov": "^3.0.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "file-loader": "^1.1.6",
    "less": "^3.0.1",
    "less-loader": "^4.0.5",
    "mocha": "^5.0.1",
    "noop-loader": "^1.0.0",
    "null-loader": "^0.1.1",
    "nyc": "^11.4.1",
    "trash": "^4.2.1",
    "trash-cli": "^1.4.0",
    "ts-loader": "^3.5.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "uglify-es": "^3.3.9",
    "uglifyjs-webpack-plugin": "^1.2.0",
    "url-loader": "^0.6.2",
    "vue-loader": "^14.1.1",
    "vue-property-decorator": "^6.0.0",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.11.0",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "pre-push": []
}
